class Link {
    let info;
    let next;
};

fn printList(let xs) {
    if (xs != nil) {
        println xs.info;
        printList(xs.next);
    } else {
        println "dine";
    }
}

let head := nil;
let tmp := nil;
let i := 1;
while (i < 10) {
    tmp := new Link();
    tmp.info := random(100);
    tmp.next := head;
    head := tmp;
    i := i + 1;
}
printList(head);